  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-native": "0.56.0",
    "react-native-drag-drop": "https://github.com/deanmcpherson/react-native-drag-drop",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "^2.7.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.0",
    "@types/react": "^16.4.6",
    "@types/react-native": "^0.56.3",
    "@types/react-native-vector-icons": "^4.6.1",
    "@types/react-navigation": "^2.0.12",
    "@types/react-redux": "^6.0.5",
